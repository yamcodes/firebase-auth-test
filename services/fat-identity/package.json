{
	"type": "module",
	"name": "fat-identity",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "pnpm build && tsc -p test/tsconfig.json && c8 tap --ts \"test/**/*.test.ts\"",
		"serve": "pnpm build && fastify start -l info dist/app.js",
		"build": "swc src --out-dir dist",
		"build:watch": "swc src --out-dir dist -w",
		"dev": "pnpm build && pnpm run '/build:watch|start/'",
		"start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"clean": "rimraf node_modules dist"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^6.0.1",
		"@fastify/sensible": "^6.0.1",
		"fastify": "^5.0.0",
		"fastify-cli": "^5.9.0",
		"fastify-plugin": "^5.0.1"
	},
	"devDependencies": {
		"@swc/cli": "0.4.1-nightly.20240914",
		"@swc/core": "^1.7.26",
		"@types/node": "^20.4.4",
		"@types/tap": "^15.0.12",
		"c8": "^7.13.0",
		"fastify-tsconfig": "^2.0.0",
		"tap": "^16.3.10",
		"ts-node": "^10.4.0",
		"tsup": "^8.0.2",
		"tsx": "^4.19.1",
		"typescript": "^5.2.2",
		"vitest": "^2.0.0"
	}
}
