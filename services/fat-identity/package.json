{
	"type": "module",
	"name": "@repo/fat-identity",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"exports": "./src/router.ts",
	"scripts": {
		"test": "pnpm build && tsc -p test/tsconfig.json && c8 tap --ts \"test/**/*.test.ts\"",
		"serve": "node dist/index.js | pino-pretty",
		"build": "rimraf dist && tsup",
		"build:watch": "tsup --watch",
		"dev": "tsx watch src/index.ts | pino-pretty",
		"start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"clean": "rimraf node_modules dist"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^6.0.1",
		"@fastify/cors": "^10.0.1",
		"@fastify/sensible": "^6.0.1",
		"@trpc/server": "10.45.2",
		"fastify": "^5.0.0",
		"fastify-cli": "^7.0.0",
		"fastify-plugin": "^5.0.1",
		"zod": "^3.23.4"
	},
	"devDependencies": {
		"@types/node": "^20.4.4",
		"@types/tap": "^15.0.12",
		"c8": "^7.13.0",
		"esbuild-plugin-file-path-extensions": "^2.1.3",
		"fastify-tsconfig": "^2.0.0",
		"pino-pretty": "^11.2.2",
		"tap": "^16.3.10",
		"ts-node": "^10.4.0",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.2.2",
		"vitest": "^2.0.0"
	}
}
