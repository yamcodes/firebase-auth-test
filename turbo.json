{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"deploy": {
			"dependsOn": ["build"]
		},
		"lint": {
			"dependsOn": ["build"]
		},
		"dev": {
			"dependsOn": ["build"],
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"test": {},
		"//#root:clean": {
			"cache": false
		},
		"//#root:emulate": {
			"cache": false,
			"persistent": true
		}
	}
}
